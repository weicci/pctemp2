--
-- Table structure for table `variable`
--

DROP TABLE IF EXISTS `variable`;
CREATE TABLE `variable` (
  `variable_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Variable Id',
  `code` varchar(255) DEFAULT NULL COMMENT 'Variable Code',
  `name` varchar(255) DEFAULT NULL COMMENT 'Variable Name',
  PRIMARY KEY (`variable_id`),
  UNIQUE KEY `VARIABLE_CODE` (`code`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='Variables';

--
-- Table structure for table `variable_value`
--

DROP TABLE IF EXISTS `variable_value`;
CREATE TABLE `variable_value` (
  `value_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Variable Value Id',
  `variable_id` int(10) unsigned NOT NULL DEFAULT '0' COMMENT 'Variable Id',
  `store_id` smallint(5) unsigned NOT NULL DEFAULT '0' COMMENT 'Store Id',
  `plain_value` text COMMENT 'Plain Text Value',
  `html_value` text COMMENT 'Html Value',
  PRIMARY KEY (`value_id`),
  UNIQUE KEY `VARIABLE_VALUE_VARIABLE_ID_STORE_ID` (`variable_id`,`store_id`),
  KEY `VARIABLE_VALUE_STORE_ID` (`store_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='Variable Value';

--
-- Table structure for table `ves_blockbuilder_block`
--

DROP TABLE IF EXISTS `ves_blockbuilder_block`;
CREATE TABLE `ves_blockbuilder_block` (
  `block_id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'Block Id',
  `title` varchar(255) NOT NULL COMMENT 'Block title',
  `alias` varchar(255) NOT NULL COMMENT 'Block Alias Key',
  `shortcode` varchar(100) NOT NULL COMMENT 'Block Shortcode',
  `show_from` timestamp NULL DEFAULT NULL COMMENT 'Profile Show From',
  `show_to` timestamp NULL DEFAULT NULL COMMENT 'Profile Show To',
  `position` int(11) NOT NULL DEFAULT '0' COMMENT 'Position',
  `status` smallint(6) NOT NULL DEFAULT '1' COMMENT 'Status',
  `created` timestamp NULL DEFAULT NULL COMMENT 'PageBuilder Creation Time',
  `modified` timestamp NULL DEFAULT NULL COMMENT 'PageBuilder Modification Time',
  `customer_group` varchar(100) NOT NULL COMMENT 'Customer Group',
  `prefix_class` varchar(100) NOT NULL COMMENT 'Prefix Class',
  `block_type` varchar(50) NOT NULL DEFAULT 'block' COMMENT 'Prefix Class',
  `page_layout` varchar(255) DEFAULT NULL COMMENT 'Page Layout',
  `layout_update_xml` text COMMENT 'Page Layout Update Content',
  `params` text NOT NULL COMMENT 'params',
  `settings` text NOT NULL COMMENT 'settings',
  `container` smallint(6) NOT NULL DEFAULT '1' COMMENT 'Is Container',
  PRIMARY KEY (`block_id`)
) ENGINE=InnoDB AUTO_INCREMENT=9 DEFAULT CHARSET=utf8 COMMENT='PageBuilder Profile';

