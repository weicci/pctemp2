<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Mgt_DeveloperToolbar::css/style.css"/>
    </head>
    <body>
        <referenceContainer name="before.body.end">
            <block class="Mgt\DeveloperToolbar\Block\Toolbar" name="mgt_developer_toolbar" template="toolbar.phtml">
                <block class="Magento\Framework\View\Element\Text\ListText" name="mgt_developer_toolbar_blocks">
                     <block class="Mgt\DeveloperToolbar\Block\Toolbar\Block\Info" name="mgt_developer_toolbar_block_info" template="block/info.phtml"/>
                     <block class="Mgt\DeveloperToolbar\Block\Toolbar\Block\MagentoVersion" name="mgt_developer_toolbar_block_magento_version" template="block/magento_version.phtml"/>
                     <block class="Mgt\DeveloperToolbar\Block\Toolbar\Block\PhpParseTime" name="mgt_developer_toolbar_block_php_parse_time" template="block/php_parse_time.phtml"/>
                     <block class="Mgt\DeveloperToolbar\Block\Toolbar\Block\MemoryConsumption" name="mgt_developer_toolbar_block_memory_consumption" template="block/memory_consumption.phtml"/>
                     <block class="Mgt\DeveloperToolbar\Block\Toolbar\Block\DatabaseQueries" name="mgt_developer_toolbar_block_database_queries" template="block/database_queries.phtml"/>
                </block>
            </block>
        </referenceContainer>
    </body>
</page>
